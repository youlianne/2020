    "by_year = movies.groupby('year')\n",
    "by_year_mean = by_year.worldwide_gross.mean()\n",
    "upper = []\n",
    "lower = []\n",
    "for year in by_year:\n",
    "    error_bar = bootstrap_CI(year[1]['worldwide_gross'],1000)\n",
    "    upper.append(error_bar[1])\n",
    "    lower.append(error_bar[0])\n",
    "upper = np.array(upper)\n",
    "lower = np.array(lower)\n",
    "errorbars = np.array([by_year_mean-lower,upper-by_year_mean])"
   ]
